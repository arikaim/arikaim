{      
    "title": "Blog",
    "name": "blog",
    "package-type": "template",
    "version": "1.2.3",
    "repository": "https://github.com/arikaim/blog-template.git",
    "icon": "blog",
    "require": {
        "library": [
            "arikaim",
            "arikaim-ui",
            "jquery",
            "fomantic"
        ],
        "extensions": [
            "blog"
        ]
    },
    "routes": [
        {
            "path": "/",         
            "home": true,  
            "page": "home",
            "language-path": true,
            "name": "Home"                   
        }
    ],
    "themes": [
        "default"
    ],
    "default-theme": "default",
    "include": {            
        "css": [
            "preflight.css",
            "style.css",
            "tailwind.min.css"
        ],
        "library": [
            "jquery",
            "arikaim",      
            "arikaim-ui"          
        ]
    }   
}